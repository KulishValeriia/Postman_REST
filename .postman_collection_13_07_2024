									"    const length = Math.floor(Math.random() * 8) + 8;\r",
									"    const charset = {\r",
									"        numbers: '0123456789',\r",
									"        uppercase: 'ABCDEFGHIJKLMNOPQRSTUVWXYZ',\r",
									"        lowercase: 'abcdefghijklmnopqrstuvwxyz'\r",
									"    };\r",
									"\r",
									"    // console.log(generatePassword());\r",
									"\r",
									"    \r",
									"    let password = '';\r",
									"    password += getRandomChar(charset.numbers);\r",
									"    password += getRandomChar(charset.uppercase);\r",
									"    password += getRandomChar(charset.lowercase);\r",
									"\r",
									"    const allChars = charset.numbers + charset.uppercase + charset.lowercase;\r",
									"    \r",
									"    for (let i = password.length; i < length; i++) {\r",
									"        password += getRandomChar(allChars);\r",
									"    }\r",
									"    \r",
									"    password = shuffleString(password);\r",
									"\r",
									"    return password;\r",
									"}\r",
									"\r",
									"function getRandomChar(chars) {\r",
									"    const randomIndex = Math.floor(Math.random() * chars.length);\r",
									"    return chars[randomIndex];\r",
									"}\r",
									"\r",
